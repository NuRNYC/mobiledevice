<p id="notice"><%= notice %></p>

<h1>Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Campus</th>
      <th>Cell</th>
      <th>Cell Type</th>
      <th>Description</th>
      <th>Email</th>
      <th>Altcontact</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.campus_id %></td>
        <td><%= ticket.cell %></td>
        <td><%= ticket.celltype %></td>
        <td><%= ticket.description %></td>
        <td><%= ticket.email %></td>
        <td><%= ticket.altcontact %></td>
        <td><%= ticket.status %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
